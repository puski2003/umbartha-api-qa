{
  "name": "backend-api",
  "version": "0.0.1",
  "description": "Umbartha API",
  "author": "Seshan Kavisanka",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "yarn run start:dev",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:local:cov": "NODE_ENV=local jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.363.0",
    "@aws-sdk/client-ses": "^3.363.0",
    "@aws-sdk/lib-storage": "^3.363.0",
    "@aws-sdk/s3-request-presigner": "^3.496.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/terminus": "^10.0.1",
    "@types/cookie-parser": "^1.4.6",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.4.0",
    "buffer-crc32": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "countries-and-timezones": "^3.6.0",
    "country-list": "^2.3.0",
    "country-to-currency": "^1.1.5",
    "currency-converter-lt": "^2.0.1",
    "date-fns": "^3.3.1",
    "date-fns-tz": "^3.1.3",
    "express-jwt": "^8.4.1",
    "geo-tz": "^8.0.2",
    "googleapis": "^144.0.0",
    "handlebars": "^4.7.8",
    "ical-generator": "^7.0.0",
    "jimp": "^0.22.12",
    "jwks-rsa": "^3.0.1",
    "jwt-decode": "^4.0.0",
    "make-random": "^3.0.2",
    "moment-timezone": "^0.5.45",
    "mongoose": "^7.5.1",
    "mustache": "^4.2.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "twilio": "^5.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.1.3",
    "@types/country-list": "^2.1.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "clover",
      "json",
      "lcov",
      "html",
      "text"
    ]
  }
}
